    containerElement={
      <div style={{ height: `100%` }} />
    }
    mapElement={
      <div style={{ height: `100%` }} />
    }
    onMapLoad={_.noop}
    onMapClick={_.noop}
    onMarkerRightClick={_.noop}
  />
</div>

		);
	}
}

export default App;
